# Place this file in your /etc/yum.repos.d/ directory

[katello-testing]
name=Katello Testing
baseurl=http://repos.fedorapeople.org/repos/katello/katello/testing/$releasever/x86_64/
enabled=1
skip_if_unavailable=1
gpgcheck=0

[katello-testing-source]
name=Katello Testing Source
baseurl=http://repos.fedorapeople.org/repos/katello/katello/testing/$$releasever/SRPMS
enabled=0
skip_if_unavailable=1
gpgcheck=0

