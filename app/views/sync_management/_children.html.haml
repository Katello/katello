-# children, parent_set
- for child in children
  - new_set = parent_set + [child[:id]]
  %tr{:id=>set_id(new_set), :class=>parent_set_class(parent_set)}
    %td
      #{child[:name]}
  = render :partial=>"children", :locals=>{:children=>child[:children], :parent_set=>new_set} if !child[:children].empty?
  = render :partial=>"repo", :collection=>child[:repos], :as=>:repo,
            :locals=>{:parent_set=>new_set} if !child[:repos].empty?
